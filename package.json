{
  "name": "aws-testing-framework-test",
  "version": "1.0.0",
  "description": "Example project demonstrating different usage patterns for the AWS Testing Framework",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js",
    "test:builtin": "cucumber-js -c cucumber-builtin.js",
    "test:custom-steps": "cucumber-js -c cucumber-custom.js",
    "test:extend-steps": "cucumber-js -c cucumber-extend.js",
    "test:feature-only": "cucumber-js -c cucumber-feature-only.js",
    "test:simple": "cucumber-js -c cucumber-simple.js",
    "test:all": "cucumber-js features/",
    "test:all-with-custom": "cucumber-js -c cucumber-all.js",
    "build": "tsc"
  },
  "keywords": [
    "aws",
    "testing",
    "examples",
    "cucumber",
    "bdd",
    "serverless",
    "integration-tests",
    "aws-testing-framework"
  ],
  "author": "Sophie Gle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sophiegle/aws-testing-framework-test.git"
  },
  "bugs": {
    "url": "https://github.com/sophiegle/aws-testing-framework-test/issues"
  },
  "homepage": "https://github.com/sophiegle/aws-testing-framework-test#readme",
  "dependencies": {
    "@types/node": "^24.0.3",
    "aws-testing-framework": "^0.6.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@cucumber/messages": "^30.1.0",
    "allure-cucumberjs": "^3.4.2",
    "assert": "^2.1.0",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=20.16.0"
  }
}
